{
    "compilerOptions": {
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "jsx": "react",
        "lib": ["ES6"],
        "moduleResolution": "node",
        "noEmit": true,
        "strict": true,
        "target": "esnext",
        "baseUrl": "./src",
        "noImplicitAny": false,
        "paths": {
            "~/*":["*"]
        },
        "experimentalDecorators":true
    },
    "exclude": ["node_modules","babel.config.js","metro.config.js","jest.config.js"]
}
